{
  "name": "scraper-backend",
  "version": "1.0.0",
  "description": "Express.js server with Puppeteer for web scraping",
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/server.ts",
    "prod": "tsc && node ./dist/server.js",
    "prettier": "npx prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": "20.11.1",
    "npm": "10.2.4"
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "node-cron": "3.0.3",
    "puppeteer": "22.11.1",
    "puppeteer-cluster": "0.24.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/node": "20.14.2",
    "husky": "^8.0.0",
    "lint-staged": "15.2.7",
    "prettier": "3.3.2",
    "prisma": "5.15.0",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  }
}
